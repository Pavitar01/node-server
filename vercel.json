{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "handle": "filesystem" },
      {
        "src": "/api/auth",
        "dest": "index.js"
      },
      {
        "src": "/api/user",
        "dest": "index.js"
      },
      {
        "src": "/api/product",
        "dest": "index.js"
      },
      {
        "src": "/api/session-logs",
        "dest": "index.js"
      },
      {
        "src": "/api/order",
        "dest": "index.js"
      },
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ]
  }
  